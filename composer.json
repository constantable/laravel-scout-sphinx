{
    "name": "constantable/laravel-scout-sphinx",
    "description": "Laravel Scout Sphinx Driver",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
      "php": ">=7.0",
      "laravel/scout": "^5.0|^6.0|^7.0|^8.0|^9.0",
      "foolz/sphinxql-query-builder": "^3.0"
    },
    "require-dev": {
      "roave/security-advisories": "dev-latest",
      "mockery/mockery": "^1.0",
      "phpunit/phpunit": "^7.5|^8|^9"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Constantable\\SphinxScout\\ServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Constantable\\SphinxScout\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Constantable\\SphinxScout\\Tests\\": "tests/"
        }
    }
}
